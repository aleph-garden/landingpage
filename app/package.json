{
  "name": "rdf-graph-viewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "oxigraph": "oxigraph serve --location data/.oxigraph-data",
    "solid": "npx -y @solid/community-server@7 -c @css:config/sparql-file-storage.json -f data/.solid-data/ -p 3000 --sparqlEndpoint http://localhost:7878/query --seedConfig config/css-seeded-pods.json",
    "dev:all": "concurrently \"npm run oxigraph\" \"npm run solid\" \"npm run dev\" --names \"oxigraph,solid,vite\" --prefix-colors \"yellow,blue,green\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@comunica/query-sparql": "^3.3.0",
    "d3": "^7.9.0",
    "n3": "^2.0.1"
  }
}
